; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "PlayerRig",
"language": "GDScript",
"path": "res://game/players/PlayerRig.gd"
} ]
_global_script_class_icons={
"PlayerRig": ""
}

[WAT]

Test_Directory="res://tests"

[application]

config/name="Metempsychosis"
run/main_scene="res://menus/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

MusicModule="*res://audio/MusicModule.tscn"
SoundModule="*res://audio/SoundModule.tscn"
JavaScriptMobile="*res://addons/LineEditJS/javascript_mobile.gd"
GameState="*res://game/autoload/game_state.gd"
SceneTransition="*res://game/autoload/SceneTransition.tscn"
MessageDisplay="*res://game/autoload/MessageDisplay.tscn"
RoomUtil="*res://game/autoload/room_util.gd"
Prompt="*res://game/autoload/Prompt.tscn"
FlashText="*res://game/autoload/FlashText.tscn"
Timeout="*res://game/autoload/Timeout.tscn"
StateEditor="*res://game/autoload/StateEditor.tscn"
WorldMap="*res://game/autoload/world_map.gd"
GameObject="*res://game/autoload/game_object.gd"

[display]

window/size/width=480
window/size/height=360
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "mixing-desk" )

